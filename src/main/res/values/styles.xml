<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 其他样式 -->

    <style name="NumberPickerWhiteText">
        <item name="android:textSize">30px</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="colorControlNormal">@android:color/transparent</item>  <!-- 添加这行，设置分割线为透明 -->
        <item name="android:divider">@android:color/transparent</item>     <!-- 添加这行，设置分割线为透明 -->
    </style>
    <style name="RoundedCornersDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@drawable/dialog_back02</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="NumberPickerStyle">
    <item name="android:textSize">25sp</item>
    <item name="android:textColorPrimary">@color/black</item>
</style>

</resources>